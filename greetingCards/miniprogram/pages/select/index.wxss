  background-color: #f6f0e7;
}

.container {
  width: 100%;
  height: 100%;
}

.container .wx-swiper-dot {
  height: 5rpx;
  width: 25rpx;
  border-radius: 0%;
  /* background-color: rgba(255, 255, 255, 0.7); */
}
.container .wx-swiper-dot-active {
  /* background-color: #ff4e5b; */
