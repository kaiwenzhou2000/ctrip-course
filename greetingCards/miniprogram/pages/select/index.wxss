.swiper {
  margin-top: 15%;
  height: 60%;
}

.swiper-item {
  display: flex;
  justify-content: center;
  align-items: center;
}

.swiper-image {
  height: 90%;
}

.tips {
  font-family: PingFangSC, sans-serif;
