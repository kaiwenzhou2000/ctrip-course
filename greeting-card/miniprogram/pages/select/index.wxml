<view class="container">
  <swiper indicator-dots="{{indicatorDots}}" current="{{current}}" bindchange="onChange" class="swiper">
    <block wx:for="{{imageUrls}}" wx:key="index">
      <swiper-item class="swiper-item">
        <image src="{{item}}" class="swiper-image" mode="aspectFit"></image>
      </swiper-item>
    </block>
  </swiper>
  <view class="tips">1.选择礼品卡</view>
  <view class="btn" bindtap="onConfirm"><view>确认</view></view>
</view>